<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="180.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OptionsController">
   <children>
      <ImageView fitHeight="300.0" fitWidth="186.0">
         <image>
            <Image url="@../among-us-background-nawpic-8.jpg" />
         </image>
      </ImageView>
      <Button layoutX="38.0" layoutY="49.0" mnemonicParsing="false" onAction="#mainMenu" text="Main Menu">
         <font>
            <Font name="Agency FB" size="24.0" />
         </font>
      </Button>
      <Button layoutX="38.0" layoutY="128.0" mnemonicParsing="false" onAction="#restart" prefHeight="45.0" prefWidth="111.0" text="Restart">
         <font>
            <Font name="Agency FB" size="24.0" />
         </font>
      </Button>
      <Button layoutX="38.0" layoutY="203.0" mnemonicParsing="false" onAction="#quit" prefHeight="45.0" prefWidth="111.0" text="Quit">
         <font>
            <Font name="Agency FB" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
