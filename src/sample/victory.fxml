<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.VictoryController">
   <children>
      <ImageView fitHeight="200.0" fitWidth="400.0" layoutX="1.0" preserveRatio="true">
         <image>
            <Image url="@../victory.png" />
         </image>
      </ImageView>
      <Button layoutX="122.0" layoutY="75.0" mnemonicParsing="false" onAction="#mainMenu" text="Main Menu">
         <font>
            <Font name="Agency FB" size="18.0" />
         </font>
      </Button>
      <Button layoutX="224.0" layoutY="75.0" mnemonicParsing="false" onAction="#quit" text="Quit">
         <font>
            <Font name="Agency FB" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
